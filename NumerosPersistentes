REM ***********************
REM
REM NUMEROS PERSISTENTES
REM
REM ***********************
REM
LET N=9
LET T=99
LET A=10
LET Q=1
LET N=N+1
IF N>T THEN 150
LET X=N
LET S=1
LET M=X/A
LET S=INT(S*A*(M-INT(M))+.5)
LET X=INT(M)
IF X=0 THEN 100
GOTO 60
IF A>S THEN 130
LET X=S
LET Q=Q+1
GOTO 50
PRINT "EL NUMERO PERSISTENTE DE" ;N; "ES" ;Q
GOTO 20
END