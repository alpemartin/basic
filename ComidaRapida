REM ***********************
REM
REM COMIDA RAPIDA
REM
REM ***********************
REM
LET F=5
LET H=5+2*F
LET C=175-F-H
IF 3*F<=H+C THEN 70
IF 3*C<=H THEN 70
PRINT "UNA HAMBURGUESA VALE" ;H/100
PRINT "UNA RACION DE PATATAS FRITAS VALE";F/100
PRINT "UNA COCA-COLA VALE";C/100
LET H=H+5
IF H>165 THEN 100
GOTO 30
LET F=F+5
IF F>165 THEN 130
GOTO 20
END