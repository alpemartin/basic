REM ***********************
REM
REM LA HORA
REM
REM ***********************
REM
LET H=0
LET H=H+1
LET M=0
IF H=13 THEN 140
LET N=H*H+M*M
LET K=N/100
LET A=INT(K)
LET B=INT(100*(K-INT(K))+.5)
IF M<>B THEN 100
IF A<>H THEN 100
PRINT "LA HORA DEL DIA ERA";K
LET M=M+1
IF M=60 THEN 130
GOTO 40
GOTO 20
END