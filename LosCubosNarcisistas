REM ***************
REM
REM LOS CUBOS NARCISISTAS
REM
REM ***************
REM
PRINT "CUAL ES EL PRIMER VALOR DE N"
INPUT N
PRINT "CUAL ES EL ULTIMO VALOR DE N"
INPUT Z
PRINT
PRINT
LET T=0
LET R=N
LET D=INT(10*(R/10-INT(R/10))+.5)
LET T=T+D*D*D
LET R=INT(R/10)
IF R=0 THEN 70
GOTO 30
IF N<>T THEN 90
PRINT "EL CUBO NARCISISTA ES" ;N
LET N=N+1
IF N>=Z THEN 120
GOTO 20 
END